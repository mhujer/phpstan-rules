{
	"name": "mhujer/phpstan-rules",
	"description": "Custom rules for phpstan",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Martin Hujer",
			"email": "mhujer@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Mhujer\\PHPStanRules\\": "src/"
		}
	},
	"autoload-dev": {
		"autoload-dev": {
			"classmap": ["tests/"]
		},
		"psr-4": {
			"Mhujer\\PHPStanRules\\": "tests/"
		}
	},
	"require": {
		"php": ">=7.1",
		"consistence/consistence": "~1.0",
		"phpstan/phpstan": "^0.9.1"
	},
	"require-dev": {
		"phpunit/phpunit": "6.5.5"
	}
}
